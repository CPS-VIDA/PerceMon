@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

include("${CMAKE_CURRENT_LIST_DIR}/PerceMonTargets.cmake")

set_and_check(PerceMon_INCLUDE_DIR
              "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_INCLUDEDIR@")
set_and_check(PerceMon_LIB_DIR "${PACKAGE_PREFIX_DIR}/@CMAKE_INSTALL_LIBDIR@")
set(PerceMon_LIBRARIES PerceMon::signalt:)
set(PerceMon_VERSION_TYPE "@PerceMon_VERSION_TYPE@")

if(NOT PerceMon_FIND_QUIETLY)
  message(
    STATUS
      "Found PerceMon: ${PerceMon_INCLUDE_DIR} (found version \"${PerceMon_VERSION}\" ${PerceMon_VERSION_TYPE})"
  )
endif()
